{
  "name": "rolldown-require",
  "type": "module",
  "version": "2.0.3",
  "description": "bundle and require a file using rolldown!",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "MIT",
  "homepage": "https://vite.icebreaker.top/packages/rolldown-require/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weapp-vite/weapp-vite.git",
    "directory": "packages/rolldown-require"
  },
  "bugs": {
    "url": "https://github.com/weapp-vite/weapp-vite/issues"
  },
  "keywords": [
    "require",
    "rolldown",
    "runtime",
    "node",
    "cjs",
    "commonjs",
    "esm",
    "typescript",
    "bundle"
  ],
  "imports": {
    "#module-sync-enabled": {
      "module-sync": "./misc/true.js",
      "default": "./misc/false.js"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "misc"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build:watch": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs"
      }
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "peerDependencies": {
    "rolldown": ">=1.0.0-rc.2"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "get-tsconfig": "^4.13.1"
  },
  "devDependencies": {
    "@types/pnpapi": "^0.0.5"
  }
}
