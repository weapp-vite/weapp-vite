{
  "name": "@wevu/api",
  "type": "module",
  "version": "0.1.1",
  "description": "tsdown bundler template",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weapp-vite/weapp-vite.git",
    "directory": "packages/weapi"
  },
  "bugs": {
    "url": "https://github.com/weapp-vite/weapp-vite/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "tsdown -w",
    "build": "tsdown",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:types": "tsd",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:sync": "tsx ./scripts/sync-support-matrix.ts",
    "docs:check": "tsx ./scripts/sync-support-matrix.ts --check",
    "prebuild": "pnpm run docs:check"
  },
  "dependencies": {
    "@douyin-microapp/typings": "^1.3.1",
    "@mini-types/alipay": "^3.0.14",
    "miniprogram-api-typings": "^5.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
