{
  "name": "@weapp-vite/web",
  "type": "module",
  "version": "1.2.4",
  "description": "Experimental web runtime for weapp-vite templates",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weapp-vite/weapp-vite.git",
    "directory": "packages/web"
  },
  "bugs": {
    "url": "https://github.com/weapp-vite/weapp-vite/issues"
  },
  "keywords": [
    "weapp",
    "web",
    "vite",
    "runtime"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs"
    },
    "./runtime/polyfill": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build:watch": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "domhandler": "^5.0.3",
    "fs-extra": "^11.3.3",
    "htmlparser2": "^10.1.0",
    "lit": "^3.3.1",
    "magic-string": "^0.30.21",
    "pathe": "^2.0.3",
    "rolldown-require": "workspace:*"
  }
}
