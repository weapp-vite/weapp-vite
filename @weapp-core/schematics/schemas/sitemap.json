{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": {},
  "description": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/sitemap.html",
  "properties": {
    "$schema": {
      "type": "string"
    },
    "rules": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "action": {
            "default": "allow",
            "enum": [
              "allow",
              "disallow"
            ],
            "type": "string"
          },
          "matching": {
            "default": "inclusive",
            "enum": [
              "exact",
              "inclusive",
              "exclusive",
              "partial"
            ],
            "type": "string"
          },
          "page": {
            "type": "string"
          },
          "params": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "priority": {
            "type": "number"
          }
        },
        "required": [
          "page"
        ],
        "type": "object"
      },
      "type": "array"
    }
  },
  "required": [
    "rules"
  ],
  "type": "object"
}
