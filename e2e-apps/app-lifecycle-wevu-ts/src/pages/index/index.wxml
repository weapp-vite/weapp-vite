<view class="page">{{message}}</view>
<view class="panel">
  <view class="panel-title">E2E Result</view>
  <view class="panel-row">Hooks: {{__e2eSummary.seen}}/{{__e2eSummary.total}}</view>
  <view class="panel-row">Skipped: {{__e2eSummary.skipped}}</view>
  <view class="panel-row">Logs: {{__e2eSummary.entries}}</view>
  <view class="panel-row">Last: {{__e2eSummary.lastHook}}</view>
</view>
<view class="panel">
  <view class="panel-title">E2E Logs (latest)</view>
  <view class="panel-row" wx:for="{{__e2ePreview}}" wx:key="order">
    {{item.order}}. {{item.hook}}{{item.skipped ? ' (skipped)' : ''}}
  </view>
</view>
