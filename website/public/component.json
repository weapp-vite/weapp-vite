{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": {},
  "description": "自定义组件配置",
  "properties": {
    "$schema": {
      "type": "string"
    },
    "component": {
      "default": true,
      "type": "boolean"
    },
    "componentFramework": {
      "type": "string"
    },
    "componentGenerics": {
      "additionalProperties": {},
      "properties": {},
      "type": "object"
    },
    "componentPlaceholder": {
      "additionalProperties": {},
      "properties": {},
      "type": "object"
    },
    "styleIsolation": {
      "default": "isolated",
      "enum": [
        "isolated",
        "apply-shared",
        "shared"
      ],
      "type": "string"
    },
    "usingComponents": {
      "additionalProperties": {
        "type": "string"
      },
      "propertyNames": {
        "type": "string"
      },
      "type": "object"
    }
  },
  "type": "object"
}
